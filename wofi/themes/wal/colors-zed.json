{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#14121090",
                "editor.background": "#14121090",
                "editor.foreground": "#c4c3c3",
                "text": "#c4c3c3",
                "text.muted": "#c4c3c370",
                "text.ignored": "#c4c3c340",
                "text.placeholder": "#c4c3c350",
                "ignored": "#c4c3c330",
                "element.hover": "#c4c3c330",
                "ghost_element.hover": "#807B8530",
                "ghost_element.selected": "#807B8530",
                "ghost_element.active": "#807B8560",
                "border": "#141210",
                "editor.highlighted_line.background": "#807B8510",
                "editor.active_line.background": "#807B8510",
                "panel.background": "#14121090",
                "title_bar.background": "#14121090",
                "title_bar.inactive_background": "#14121090",
                "status_bar.background": "#14121090",
                "drop_target.background": "#14121090",
                "elevated_surface.background": "#141210",
                "toolbar.background": "#14121090",
                "tab_bar.background": "#14121090",
                "tab.inactive_background": "#14121090",
                "tab.active_background": "#807B8530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#141210",
                "scrollbar.thumb.background": "#807B85",
                "editor.gutter.background": "#14121090",
                "terminal.background": "#14121010",
                "terminal.foreground": "#c4c3c3",
                "terminal.dim_foreground": "#c4c3c3",
                "terminal.bright_foreground": "#c4c3c3",
                "terminal.ansi.black": "#141210",
                "terminal.ansi.red": "#807B85",
                "terminal.ansi.green": "#75B68D",
                "terminal.ansi.yellow": "#ADA09F",
                "terminal.ansi.blue": "#D4BEA0",
                "terminal.ansi.magenta": "#86D69B",
                "terminal.ansi.cyan": "#B3D58F",
                "terminal.ansi.white": "#c4c3c3",
                "terminal.ansi.bright_black": "#6e615a",
                "terminal.ansi.bright_red": "#807B85",
                "terminal.ansi.bright_green": "#75B68D",
                "terminal.ansi.bright_yellow": "#ADA09F",
                "terminal.ansi.bright_blue": "#D4BEA0",
                "terminal.ansi.bright_magenta": "#86D69B",
                "terminal.ansi.bright_cyan": "#B3D58F",
                "terminal.ansi.bright_white": "#c4c3c3",
                "modified": "#807B85",
                "syntax": {
                    "attribute": {
                        "color": "#807B85"
                    },
                    "boolean": {
                      "color": "#75B68D"
                    },
                    "comment": {
                      "color": "#6e615a"
                    },
                    "comment.doc": {
                      "color": "#6e615a"
                    },
                    "constant": {
                      "color": "#75B68D"
                    },
                    "function": {
                      "color": "#B3D58F"
                    },
                    "keyword": {
                      "color": "#D4BEA0"
                    },
                    "number": {
                      "color": "#86D69B"
                    },
                    "operator": {
                      "color": "#D4BEA0"
                    },
                    "string": {
                      "color": "#807B85"
                    },
                    "variable": {
                      "color": "#75B68D"
                    }
                },
                "players": [
                  {
                    "cursor": "#807B85",
                    "background": "#141210",
                    "selection": "#807B8530"
                  }
                ]
            }
        }
    ]
}
