{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#110f0d90",
                "editor.background": "#110f0d90",
                "editor.foreground": "#c3c3c2",
                "text": "#c3c3c2",
                "text.muted": "#c3c3c270",
                "text.ignored": "#c3c3c240",
                "text.placeholder": "#c3c3c250",
                "ignored": "#c3c3c230",
                "element.hover": "#c3c3c230",
                "ghost_element.hover": "#30483730",
                "ghost_element.selected": "#30483730",
                "ghost_element.active": "#30483760",
                "border": "#110f0d",
                "editor.highlighted_line.background": "#30483710",
                "editor.active_line.background": "#30483710",
                "panel.background": "#110f0d90",
                "title_bar.background": "#110f0d90",
                "title_bar.inactive_background": "#110f0d90",
                "status_bar.background": "#110f0d90",
                "drop_target.background": "#110f0d90",
                "elevated_surface.background": "#110f0d",
                "toolbar.background": "#110f0d90",
                "tab_bar.background": "#110f0d90",
                "tab.inactive_background": "#110f0d90",
                "tab.active_background": "#30483730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#110f0d",
                "scrollbar.thumb.background": "#304837",
                "editor.gutter.background": "#110f0d90",
                "terminal.background": "#110f0d10",
                "terminal.foreground": "#c3c3c2",
                "terminal.dim_foreground": "#c3c3c2",
                "terminal.bright_foreground": "#c3c3c2",
                "terminal.ansi.black": "#110f0d",
                "terminal.ansi.red": "#304837",
                "terminal.ansi.green": "#46523E",
                "terminal.ansi.yellow": "#4E6852",
                "terminal.ansi.blue": "#985C3E",
                "terminal.ansi.magenta": "#57966D",
                "terminal.ansi.cyan": "#A49D6F",
                "terminal.ansi.white": "#c3c3c2",
                "terminal.ansi.bright_black": "#6c6558",
                "terminal.ansi.bright_red": "#304837",
                "terminal.ansi.bright_green": "#46523E",
                "terminal.ansi.bright_yellow": "#4E6852",
                "terminal.ansi.bright_blue": "#985C3E",
                "terminal.ansi.bright_magenta": "#57966D",
                "terminal.ansi.bright_cyan": "#A49D6F",
                "terminal.ansi.bright_white": "#c3c3c2",
                "modified": "#304837",
                "syntax": {
                    "attribute": {
                        "color": "#304837"
                    },
                    "boolean": {
                      "color": "#46523E"
                    },
                    "comment": {
                      "color": "#6c6558"
                    },
                    "comment.doc": {
                      "color": "#6c6558"
                    },
                    "constant": {
                      "color": "#46523E"
                    },
                    "function": {
                      "color": "#A49D6F"
                    },
                    "keyword": {
                      "color": "#985C3E"
                    },
                    "number": {
                      "color": "#57966D"
                    },
                    "operator": {
                      "color": "#985C3E"
                    },
                    "string": {
                      "color": "#304837"
                    },
                    "variable": {
                      "color": "#46523E"
                    }
                },
                "players": [
                  {
                    "cursor": "#304837",
                    "background": "#110f0d",
                    "selection": "#30483730"
                  }
                ]
            }
        }
    ]
}
