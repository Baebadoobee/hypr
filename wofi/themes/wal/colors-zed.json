{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#08050590",
                "editor.background": "#08050590",
                "editor.foreground": "#c1c0c0",
                "text": "#c1c0c0",
                "text.muted": "#c1c0c070",
                "text.ignored": "#c1c0c040",
                "text.placeholder": "#c1c0c050",
                "ignored": "#c1c0c030",
                "element.hover": "#c1c0c030",
                "ghost_element.hover": "#724B4B30",
                "ghost_element.selected": "#724B4B30",
                "ghost_element.active": "#724B4B60",
                "border": "#080505",
                "editor.highlighted_line.background": "#724B4B10",
                "editor.active_line.background": "#724B4B10",
                "panel.background": "#08050590",
                "title_bar.background": "#08050590",
                "title_bar.inactive_background": "#08050590",
                "status_bar.background": "#08050590",
                "drop_target.background": "#08050590",
                "elevated_surface.background": "#080505",
                "toolbar.background": "#08050590",
                "tab_bar.background": "#08050590",
                "tab.inactive_background": "#08050590",
                "tab.active_background": "#724B4B30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#080505",
                "scrollbar.thumb.background": "#724B4B",
                "editor.gutter.background": "#08050590",
                "terminal.background": "#08050510",
                "terminal.foreground": "#c1c0c0",
                "terminal.dim_foreground": "#c1c0c0",
                "terminal.bright_foreground": "#c1c0c0",
                "terminal.ansi.black": "#080505",
                "terminal.ansi.red": "#724B4B",
                "terminal.ansi.green": "#9D2929",
                "terminal.ansi.yellow": "#996464",
                "terminal.ansi.blue": "#946B6B",
                "terminal.ansi.magenta": "#AD7F80",
                "terminal.ansi.cyan": "#B48C8C",
                "terminal.ansi.white": "#c1c0c0",
                "terminal.ansi.bright_black": "#665353",
                "terminal.ansi.bright_red": "#724B4B",
                "terminal.ansi.bright_green": "#9D2929",
                "terminal.ansi.bright_yellow": "#996464",
                "terminal.ansi.bright_blue": "#946B6B",
                "terminal.ansi.bright_magenta": "#AD7F80",
                "terminal.ansi.bright_cyan": "#B48C8C",
                "terminal.ansi.bright_white": "#c1c0c0",
                "modified": "#724B4B",
                "syntax": {
                    "attribute": {
                        "color": "#724B4B"
                    },
                    "boolean": {
                      "color": "#9D2929"
                    },
                    "comment": {
                      "color": "#665353"
                    },
                    "comment.doc": {
                      "color": "#665353"
                    },
                    "constant": {
                      "color": "#9D2929"
                    },
                    "function": {
                      "color": "#B48C8C"
                    },
                    "keyword": {
                      "color": "#946B6B"
                    },
                    "number": {
                      "color": "#AD7F80"
                    },
                    "operator": {
                      "color": "#946B6B"
                    },
                    "string": {
                      "color": "#724B4B"
                    },
                    "variable": {
                      "color": "#9D2929"
                    }
                },
                "players": [
                  {
                    "cursor": "#724B4B",
                    "background": "#080505",
                    "selection": "#724B4B30"
                  }
                ]
            }
        }
    ]
}
